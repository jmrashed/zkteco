{
    "name": "jmrashed/zkteco",
    "version": "1.3.0",
    "description": "ZKTeco Package For Laravel. This package provides seamless integration with ZKTeco devices within Laravel applications, enabling communication with attendance devices such as fingerprint, face recognition, or RFID using UDP protocol.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Md Rasheduzzaman",
            "email": "jmrashed@gmail.com"
        }
    ],
    "autoload": {
      "psr-4":{
          "Jmrashed\\Zkteco\\": "src"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^6.0"
    },
    "provides": {
        "jmrashed/zkteco": "1.3.0"
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "suggests": {
    }
}
